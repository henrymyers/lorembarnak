  "repository": {
    "type": "git",
    "url": "https://github.com/henrymyers/lorembarnak.git"
  },
  "bugs": {
    "url": "https://github.com/henrymyers/lorembarnak/issues"
  },
  "homepage": "https://github.com/henrymyers/lorembarnak#readme",
  "devDependencies": {
    "browserify": "^16.2.3",
    "gh-pages": "^2.0.1",
    "pre-commit": "^1.2.2",
    "typescript": "^3.2.4"
  }
}
