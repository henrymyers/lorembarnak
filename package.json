{
  "name": "lorembarnak",
  "version": "1.0.1",
  "description": "The Québécois swear generator",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc",
    "build:gh": "npm run build && browserify ./docs/example.js -o ./docs/assets/bundle.js",
